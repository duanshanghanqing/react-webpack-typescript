  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/basarat/typescript-react.git"
  },
  "scripts": {
    "start": "webpack-dev-server --config ./build/webpack.dev.server.config.js",
    "build:dev": "npm run rm && webpack --config ./build/webpack.dev.config.js",
